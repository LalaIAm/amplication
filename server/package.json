{
  "name": "@familyr/server",
  "private": true,
  "scripts": {
    "start": "nest start",
    "start:watch": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "build": "nest build",
    "test": "jest",
    "seed": "ts-node scripts/seed.ts",
    "db:migrate-save": "prisma migrate dev",
    "db:migrate-up": "prisma migrate deploy",
    "db:clean": "ts-node scripts/clean.ts",
    "db:init": "run-s \"db:migrate-save -- --name 'initial version'\" db:migrate-up seed",
    "prisma:generate": "prisma generate",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "docker:build": "docker build ."
  },
  "dependencies": {
    "@nestjs/common": "8.4.4",
    "@nestjs/config": "2.0.0",
    "@nestjs/core": "8.4.4",
    "@nestjs/graphql": "10.0.8",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/passport": "8.2.1",
    "@nestjs/platform-express": "8.4.4",
    "@nestjs/serve-static": "2.2.2",
    "@nestjs/swagger": "5.2.1",
    "@prisma/client": "3.12.0",
    "apollo-server-express": "3.6.7",
    "bcrypt": "5.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "graphql": "16.3.0",
    "graphql-type-json": "0.3.2",
    "nest-access-control": "2.2.0",
    "nest-morgan": "2.0.0",
    "nestjs-prisma": "0.15.1",
    "npm-run-all": "4.1.5",
    "passport": "0.5.2",
    "passport-http": "0.3.0",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "swagger-ui-express": "4.3.0",
    "ts-node": "10.7.0"
  },
  "devDependencies": {
    "@nestjs/cli": "8.2.5",
    "@nestjs/testing": "8.4.4",
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.13",
    "@types/graphql-type-json": "0.3.2",
    "@types/jest": "27.4.1",
    "@types/normalize-path": "3.0.0",
    "@types/passport-http": "0.3.9",
    "@types/passport-jwt": "3.0.6",
    "@types/supertest": "2.0.12",
    "jest": "27.5.1",
    "jest-mock-extended": "^2.0.5",
    "prisma": "3.12.0",
    "supertest": "6.2.2",
    "ts-jest": "27.1.4",
    "type-fest": "2.12.2",
    "typescript": "4.6.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  }
